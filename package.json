{
  "name": "api-template",
  "version": "0.0.3",
  "description": "API template",
  "author": "Alen Inal-Ipa",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:all": "npm run socket:docs:build && nest build",
    "format": "prettier --write \\\"src/**/*.ts\\\" \\\"test/**/*.ts\\\"",
    "start": "node dist/src/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "docs": "typedoc --options typedoc.json",
    "test": "jest",
    "socket:docs:build": "rimraf ./docs && apidoc --single -i ./src -o ./docs",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "commit": "git-cz",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ormconfig.ts",
    "schema:sync": "ts-node ./node_modules/.bin/typeorm schema:sync",
    "migration": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert"
  },
  "apidoc": {
    "name": "API Template",
    "version": "1.0.0",
    "description": "Socket api",
    "title": "Socket API",
    "url": "/api/v1",
    "sampleUrl": "/api/v1"
  },
  "dependencies": {
    "@liaoliaots/nestjs-redis": "6.0.0",
    "@nestjs-modules/mailer": "1.7.1",
    "@nestjs/common": "8.0.6",
    "@nestjs/config": "1.0.1",
    "@nestjs/core": "8.0.6",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.2.1",
    "@nestjs/platform-express": "8.0.6",
    "@nestjs/platform-socket.io": "8.0.6",
    "@nestjs/swagger": "5.0.9",
    "@nestjs/testing": "8.0.6",
    "@nestjs/typeorm": "8.0.2",
    "@types/aws4": "1.11.1",
    "apidoc": "0.54.0",
    "aws4": "1.11.0",
    "bcrypt": "5.0.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "ejs": "3.1.8",
    "express": "4.17.1",
    "ioredis": "4.28.5",
    "jsonapi-serializer": "3.6.7",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "moment": "2.29.3",
    "multer": "1.4.4",
    "nest-router": "1.0.9",
    "nestjs-i18n": "9.1.2",
    "nestjs-s3": "1.0.1",
    "nodemailer": "6.7.5",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pg": "8.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.3.0",
    "socket.io": "4.6.0",
    "socket.io-redis": "6.1.1",
    "swagger-ui-express": "4.1.6",
    "typeorm": "0.2.37"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.1",
    "@nestjs/schematics": "8.0.2",
    "@types/apidoc": "0.50.0",
    "@types/bcrypt": "3.0.0",
    "@types/ejs": "3.1.1",
    "@types/express": "4.17.13",
    "@types/ioredis": "4.28.8",
    "@types/jest": "27.0.1",
    "@types/jsonapi-serializer": "3.6.4",
    "@types/lodash": "4.14.168",
    "@types/multer": "1.4.7",
    "@types/node": "16.6.1",
    "@types/passport-jwt": "3.0.6",
    "@types/passport-local": "1.0.34",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.0",
    "jest": "27.0.6",
    "prettier": "2.3.2",
    "socket.io-client": "4.6.0",
    "supertest": "6.1.5",
    "ts-jest": "27.0.5",
    "ts-loader": "9.2.5",
    "ts-node": "10.2.0",
    "tsconfig-paths": "3.10.1",
    "typedoc": "0.21.5",
    "typescript": "4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "_moduleAliases": {
    "@filters": "./dist/src/common/filters",
    "@guards": "./dist/src/common/guards",
    "@interfaces": "./dist/src/common/interfaces",
    "@enums": "./dist/src/common/enums",
    "@interceptors": "./dist/src/common/interceptors",
    "@decorators": "./dist/src/common/decorators",
    "@docs": "./dist/src/common/docs",
    "@v1": "./dist/src/routes/v1",
    "@common": "./dist/src/common"
  },
  "engines": {
    "npm": "^8.0.0",
    "node": "^16.0.0"
  }
}
